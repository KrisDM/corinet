<?xml version="1.0" encoding="UTF-8"?>
<typeDefaults xmlns="http://www.corinet.org/namespaces/CORINETSetup">
	<default types="conj2">
		<siteParams>
			<param name="alphaStep" value="0.25"/>
			<param name="alphaMax" value="6"/>
			<param name="learningRate" value="1"/>
		</siteParams>
	</default>
	<default types="conj2">
		<normalisation type="normNode">
			<param name="posNorm" value="1.0"/>
			<param name="posConstrain" value="1.0"/>
			<param name="negNorm" value="-1.0"/>
			<param name="negConstrain" value="-1.0"/>
		</normalisation>
	</default>
	<default numSites="1">
		<outputFunction type="addSites">
			<attenuation type="clipLow"/>
		</outputFunction>
	</default>
</typeDefaults>
