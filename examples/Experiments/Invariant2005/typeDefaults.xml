<?xml version="1.0" encoding="UTF-8"?>
<typeDefaults xmlns="http://www.corinet.org/namespaces/CORINETSetup">
	<default types="discrete">
		<netParams>
			<param name="pathLength" value="2"/>
		</netParams>
	</default>
	<default types="conj2 disj1 disj2">
		<initialisation type="value"/>
	</default>
	<default types="conj2">
		<normalisation type="normNode"/>
	</default>
	<default types="disj1 disj2">
		<normalisation type="normInput"/>
	</default>
	<default types="conj2">
		<siteParams>
			<param name="alphaStep" value="0.25"/>
			<param name="alphaMax" value="10"/>
		</siteParams>
	</default>
	<default types="conj2">
		<siteParams>
			<param name="learningRate" value="1"/>
			<param name="negLearningRate" value="1"/>
		</siteParams>
	</default>
	<default types="disj1 disj2">
		<siteParams>
			<param name="learningRate" value="0.25"/>
		</siteParams>
	</default>
	<default numSites="1">
		<outputFunction type="addSites">
			<noiseFunction type="multiplicative">
				<randGen type="log1">
					<param name="high" value="0.01"/>
				</randGen>
			</noiseFunction>
		</outputFunction>
	</default>
</typeDefaults>
